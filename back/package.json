{
  "name": "conciencia-backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint .",
    "setup": "node setup-env.js",
    "configure": "node configure-supabase.js",
    "fix-db": "node fix-database-url.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:push": "npx prisma db push",
    "db:generate": "npx prisma generate",
    "test:connection": "ts-node src/test-connection.ts",
    "test:login": "ts-node src/test-login.ts",
    "test:local": "ts-node src/test-connection-local.ts",
    "test:supabase": "ts-node src/test-supabase-simple.ts",
    "test:supabase-direct": "ts-node src/test-supabase-direct.ts",
    "test:supabase-auth": "ts-node src/test-supabase-auth.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/pg": "^8.15.5",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "prisma": "^5.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.14.9",
    "eslint": "^9.9.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
